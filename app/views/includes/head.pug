doctype html
html
	head
		meta(charset='utf-8')
		meta(
			name='viewport'
			content='width=device-width, initial-scale=1, shrink-to-fit=no'
		)
		
		if sublink
			link(
				rel='stylesheet'
				type='text/css' 
				href='../css/bootstrap.min.css'
			)
		else
			link(
				rel='stylesheet'
				type='text/css' 
				href='css/bootstrap.min.css'
			)

		if sublink
			link(
				rel="stylesheet" 
				type="text/css"
				href="../css/style.css"
			)
		else
			link(
				rel="stylesheet" 
				type="text/css"
				href="css/style.css"
			)
		title= 'Voteapp-5000'

	body
